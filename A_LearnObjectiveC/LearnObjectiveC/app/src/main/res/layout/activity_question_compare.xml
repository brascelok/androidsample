<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_questioncompare"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.blackrose.learnobjectivec.activity.QuestionCompareActivity">

    <com.google.android.gms.ads.AdView
        android:id="@+id/adViewQuestionCompare"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-2369843568104317/6205692589"/>

    <ListView
        android:id="@+id/lv_all_question"
        android:layout_width="match_parent"
        android:layout_above="@id/adViewQuestionCompare"
        android:layout_height="match_parent" />


</RelativeLayout>
